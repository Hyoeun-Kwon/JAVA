package com.javalec.base;

public class Main {

	public static void main(String[] args) {
		// 1부터 100까지의 합을 구하기
//		int tot = 0;
//		for(int i=1; i<=100; i++) {
//			tot+=1;
//		}
//		System.out.println(tot);
		
		// 1부터 100까지의 합을 구하기 while(범위 없음)
		int tot=0;
		int i =1;
		while(i<=100) {    //물어보는것: i가 100보다 작냐
			tot+=i;
			i++;
		}
		
		
	}

}
