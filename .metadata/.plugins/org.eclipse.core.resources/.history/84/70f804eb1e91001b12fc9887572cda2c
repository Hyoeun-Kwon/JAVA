package com.javalec.base;

public class MainClass {

	public static void main(String[] args) {
		// 비교문 #1
		int i = 10; // ~를 위한 변수다 라고 옆에 적어두는 게 좋음
		int j = 20;
		int k = 10;
		
		
		if( i> j ) {    //{: (scope) 범위 중요! true 일때만 들어감
			
			System.out.println("i가 j보다 큽니다");
			
		}
		
		System.out.println(">>> End <<<");
		
	}

}
